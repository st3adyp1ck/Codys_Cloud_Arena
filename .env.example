# Application Settings
VITE_APP_TITLE=Cody's Cloud Arena
VITE_APP_DESCRIPTION=Build, battle, and upgrade robots powered by Alibaba Cloud technologies

# API Configuration
VITE_API_URL=http://localhost:3000/api

# Alibaba Cloud Configuration
ALIBABA_CLOUD_ACCESS_KEY_ID=your-access-key-id
ALIBABA_CLOUD_ACCESS_KEY_SECRET=your-access-key-secret
ALIBABA_CLOUD_REGION=your-region

# OSS Configuration
VITE_OSS_BUCKET=your-oss-bucket
VITE_OSS_ENDPOINT=your-oss-endpoint
VITE_OSS_DOMAIN=your-oss-domain

# CDN Configuration
VITE_CDN_DOMAIN=your-cdn-domain

# RDS Configuration
DB_HOST=your-rds-host
DB_PORT=3306
DB_NAME=codys_cloud_arena
DB_USER=game_user
DB_PASSWORD=your-secure-password

# Function Compute Configuration
VITE_FUNCTION_COMPUTE_SERVICE=your-fc-service
VITE_FUNCTION_COMPUTE_REGION=your-fc-region

# Analytics and Monitoring
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=your-analytics-id
